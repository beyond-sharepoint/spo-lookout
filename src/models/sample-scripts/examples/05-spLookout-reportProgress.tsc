import { spLookout } from 'sp-lookout';
import * as bluebird from 'bluebird';

const runner = async () => {
    spLookout.reportProgress("Starting...");
    await bluebird.delay(1000);
    spLookout.reportProgress("Formatting C:\\>...");
    await bluebird.delay(1000);
    spLookout.reportProgress("Just Kidding...");
    await bluebird.delay(1000);
    spLookout.reportProgress("Or was I...");
    await bluebird.delay(1000);
    return "--fin--";
};

export default runner();