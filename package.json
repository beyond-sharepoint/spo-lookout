  "name": "sp-lookout",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.0.0",
    "@microsoft/sp-client-base": "^1.0.0",
    "@microsoft/sp-core-library": "^1.1.0",
    "@types/bluebird": "^3.5.8",
    "@types/core-js": "^0.9.42",
    "@types/jest": "^20.0.2",
    "@types/lodash": "^4.14.68",
    "@types/mousetrap": "^1.5.34",
    "@types/node": "^8.0.7",
    "@types/react": "^15.0.34",
    "@types/react-dom": "^15.5.1",
    "@types/react-grid-layout": "^0.14.2",
    "@types/react-router": "^4.0.12",
    "@types/urijs": "^1.15.33",
    "babel-standalone": "^6.25.0",
    "font-awesome": "^4.7.0",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "mobx": "^3.1.16",
    "mobx-react": "^4.2.2",
    "moment": "^2.18.1",
    "monaco-editor": "^0.9.0",
    "mousetrap": "^1.6.1",
    "node-sass-chokidar": "^0.0.3",
    "office-ui-fabric-react": "^4.14.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-grid-layout": "^0.14.6",
    "react-inspector": "^2.1.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "typescript": "^2.4.1",
    "urijs": "^1.18.10",
    "whatwg-fetch": "^2.0.3",
    "xlsx": "^0.10.6"
  },
  "devDependencies": {
    "cpr": "^2.2.0",
    "npm-run-all": "^4.0.2",
    "react-scripts-ts": "2.3.2"
  },
  "scripts": {
    "postinstall": "cd hostwebproxy && npm install && cd .. && cpr node_modules/monaco-editor/min/vs public/vs -o",
    "build-proxy": "cd hostwebproxy && npm run build && cd ..",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts-ts start",
    "build": "npm run build-proxy && npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  }
}
