    "babel-preset-es2015": "^6.24.1",
    "cpr": "^2.2.0",
    "html-webpack-inline-source-plugin": "^0.0.9",
    "html-webpack-plugin": "^2.29.0",
    "raw-loader": "^0.5.1",
